{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-bcmath": "*",
        "ext-openssl": "*",
        "ext-redis": "*",
        "bavix/laravel-wallet": "7.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^3.0",
        "geoip2/geoip2": "^2.12",
        "google/protobuf": "^3.21",
        "grpc/grpc": "^1.42",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "jenssegers/mongodb": "3.8",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "maatwebsite/excel": "^3.1",
        "messagebird/php-rest-api": "^3.1",
        "mews/captcha": "^3.2",
        "predis/predis": "^2.0",
        "spatie/laravel-permission": "^5.5",
        "tymon/jwt-auth": "^1.0",
        "workerman/gateway-worker": "^3.0",
        "ext-grpc": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Bavix\\": "app/overrides/",
            "Backstage\\": "protobuf/compile/Backstage",
            "GPBMetadata\\Lobby\\Backstage\\": "protobuf/compile/GPBMetadata/Lobby/Backstage"
        },
        "files" : [
            "app/helpers.php"
        ],
        "exclude-from-classmap": [
            "vendor\\bavix\\laravel-wallet\\src\\Internal\\Dto\\TransactionDtoInterface.php",
            "vendor\\bavix\\laravel-wallet\\src\\Internal\\Dto\\TransactionDto.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
